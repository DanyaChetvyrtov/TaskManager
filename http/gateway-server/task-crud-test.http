@path = http://localhost:8072/task/api/v1/task
#@accountId = f3ed0ff6-faec-4b9e-860d-0903eccf28b3
@accountId = 4e4d7031-9ede-4968-b1dd-90b8d95d2a38
@taskId = 8bdde637-773a-4968-be78-de2d671648b6

### GET all tasks by account id
GET {{path}}?accountId={{accountId}}

### GET by id
GET {{path}}/{{taskId}}

### POST create new account
POST {{path}}
Content-Type: application/json
Accept: application/json

{
  "accountId":  "{{accountId}}",
  "title": "Test",
  "body": "Test",
  "completed": false
}

### PUT update account
PUT {{path}}/{{taskId}}
Content-Type: application/json

{
  "taskId": "{{taskId}}",
  "accountId":  "{{accountId}}",
  "title": "Test UPDATED",
  "body": "Test UPDATED",
  "completed": true
}

### DELETE by id
DELETE {{path}}/{{taskId}}
